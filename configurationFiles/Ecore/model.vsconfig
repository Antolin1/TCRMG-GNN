import epackage	"inputs/smallEcore.ecore"
import viatra	"src/queries/constraints.vql"

generate {
	metamodel = { package smallEcore excluding { ENamedElement.name } }
	constraints = { package queries }
	solver = ViatraSolver
	scope = {
		#node += 12..12
	}
	
	config = {
		runtime = 120,
		log-level = normal,
		"path" = "/usr/bin/z3"
	}
	
	runs = 1
	number = 1
	
	debug =			"outputs/debug"
	log =			"outputs/log.txt"
	output =		"outputs/models"
}
