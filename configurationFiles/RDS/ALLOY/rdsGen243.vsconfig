import epackage	"inputs/rdsSimplified.ecore"
import viatra	"src/queries/constraints.vql"

generate {
	metamodel = { package rdsSimplified }
	constraints = { package queries }
	solver = AlloySolver
	scope = {
		#node += 12,
		#<rdsSimplified::Database> += 1..1
	}
	
	config = {
		runtime = 120,
		log-level = normal,
		"path" = "/usr/bin/z3"
	}
	
	runs = 1
	number = 30
	
	debug =			"outputs243/debug"
	log =			"outputs243/log.txt"
	output =		"outputs243/models"
}
